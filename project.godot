; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tune-Up Racing"
run/main_scene="res://Levels/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Tune-Up Racing"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.105882, 0.105882, 0.105882, 1)
boot_splash/show_image=false
config/icon="res://Textures/OS/Icon.png"
config/windows_native_icon="res://Textures/OS/Icon.ico"

[autoload]

Utils="*res://Game_Logic/utils.gd"
Save_Load="*res://Save_Game/Save_Load.gd"
SelectedScene="*res://UI/Selected_Scene.gd"
FontColorSettings="*res://UI/Fonts/Font_Color_Settings.gd"
AssetList="*res://Assets/Algorithms/asset_list.tscn"
Laptime="*res://Game_Logic/laptime.gd"
Placing="*res://Game_Logic/placing.gd"
RaceStatus="*res://Game_Logic/race_status.gd"
Colors="*res://Libraries/colors.gd"
Settings="*res://Game_Logic/settings.gd"
Clock="*res://Game_Logic/clock.gd"
CarMarket="*res://UI/Computer/car_market.gd"
Sound="*res://Sound/Volume Control/Sound.tscn"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=3
window/size/initial_position=Vector2i(1280, 1100)
window/stretch/mode="canvas_items"
window/quality/use_taa=true

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_sfxr/plugin.cfg", "res://addons/rmsmartshape/plugin.cfg")

[gui]

theme/custom="res://UI/Garage.tres"
theme/custom_font="res://UI/Garage.tres"

[importer_defaults]

mp3={
"loop": true
}

